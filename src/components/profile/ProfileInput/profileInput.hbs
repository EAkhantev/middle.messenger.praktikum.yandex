<li class="user-data-item">
  <label for="{{ name }}">{{ labelValue }}</label>
  <input 
    class="input-profile" 
    type="{{ type }}" 
    id="{{ name }}" 
    name="{{ name }}"
    {{#if defaultValue}} 
      value="{{ defaultValue }}"
    {{/if}}
    {{#if isDisable}} 
      disabled=""
    {{/if}}
    {{#if autocomplete}} 
      autocomplete="{{ autocomplete }}" 
    {{/if}}
  >
</li>
